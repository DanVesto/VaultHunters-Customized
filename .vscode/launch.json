    "version": "0.2.0",
    "configurations": [
      {
          "name": "packwizPush",
          "type": "PowerShell",
          "request": "launch",
          "script": "./push.bat ${input:version}",
          "cwd": "${cwd}"
      },
      {
          "name": "packwizCmd",
          "type": "PowerShell",
          "request": "launch",
          "script": "./packwiz ${input:packwizArguments}",
          "cwd": "${cwd}"
      }
    ],
    "inputs": [
      {
        "id": "version",
        "type": "promptString",
        "description": "Version for packwiz",
        "default": "1.0.2" //REPLACEME version
      },
      {
        "id": "packwizArguments",
        "type": "promptString",
        "description": "Arguments for packwiz command",
        "default": "cf add "
      }
    ]
}